{
  "name": "feed-finder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check --no-errors-on-unmatched --files-ignore-unknown=true src",
    "test": "vitest",
    "test:run": "vitest run",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy --message \"Deployed from GitHub Actions: ${GITHUB_WORKFLOW} (${GITHUB_RUN_NUMBER})\" --commit-hash \"${GITHUB_SHA}\" --commit-message \"${GITHUB_COMMIT_MESSAGE}\"",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "neverthrow": "^8.2.0",
    "node-html-parser": "^7.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@cloudflare/vite-plugin": "^1.9.6",
    "@mizchi/lsmcp": "^0.9.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript/native-preview": "^7.0.0-dev.20250810.1",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "terser": "^5.43.1",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.0"
  }
}
